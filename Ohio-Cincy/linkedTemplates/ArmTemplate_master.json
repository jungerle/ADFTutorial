{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Ohio-Cincy"
		},
		"dl_covidjsudatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'dl_covidjsudatalake'"
		},
		"ls_covidjsusubscription_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_covidjsusubscription'"
		},
		"dl_covidjsudatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidjsudatalake.dfs.core.windows.net/"
		},
		"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().srcBaseURL}"
		},
		"tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_srcRelativeURL": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/main/ecdc_data/hospital_admissions.csv"
		},
		"tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"tr_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f952409b-38e3-4a85-902f-6ada944dfd99/resourceGroups/covid-mapping-rg/providers/Microsoft.Storage/storageAccounts/covidjsusubscription"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Ohio-Cincy_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"dl_covidjsudatalake_accountKey": {
						"value": "[parameters('dl_covidjsudatalake_accountKey')]"
					},
					"ls_covidjsusubscription_connectionString": {
						"value": "[parameters('ls_covidjsusubscription_connectionString')]"
					},
					"dl_covidjsudatalake_properties_typeProperties_url": {
						"value": "[parameters('dl_covidjsudatalake_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_srcRelativeURL": {
						"value": "[parameters('tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_srcRelativeURL')]"
					},
					"tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName": {
						"value": "[parameters('tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName')]"
					},
					"tr_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}